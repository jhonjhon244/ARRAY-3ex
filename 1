#include <iostream>
using namespace std;

int main() {
    int num[5];
    int soma = 0;

    for(int i = 0; i < 5; i++) {
        cout << "Digite o valor para a posição " << i + 1 << ": ";
        cin >> num[i];
    }

    for(int i = 0; i < 5; i++) {
        soma += num[i];
    }

    cout << "A soma dos valores é: " << soma << endl;

    return 0;
}
